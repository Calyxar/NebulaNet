{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": null,
  "github": { "silent": true },
  "regions": ["iad1"],
  "public": false,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/privacy$", "dest": "/privacy.html" },
    { "src": "/privacy/(.*)", "dest": "/privacy.html" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
